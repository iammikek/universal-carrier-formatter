{
  "name": "Example Regional Courier",
  "base_url": "https://api.examplecourier.com",
  "version": "v1",
  "description": "Example regional courier API for tracking and shipping",
  "endpoints": [
    {
      "path": "/api/v1/track/{tracking_number}",
      "method": "GET",
      "summary": "Track a shipment",
      "description": "Retrieve tracking information for a shipment using its tracking number",
      "authentication_required": true,
      "tags": ["tracking"],
      "request": {
        "content_type": "application/json",
        "parameters": [
          {
            "name": "tracking_number",
            "type": "string",
            "location": "path",
            "required": true,
            "description": "The carrier's tracking number",
            "example": "1Z999AA10123456784"
          }
        ]
      },
      "responses": [
        {
          "status_code": 200,
          "content_type": "application/json",
          "description": "Successful tracking response",
          "body_schema": {
            "type": "object",
            "properties": {
              "tracking_number": {
                "type": "string"
              },
              "status": {
                "type": "string",
                "enum": ["in_transit", "delivered", "exception"]
              },
              "current_location": {
                "type": "object",
                "properties": {
                  "city": {"type": "string"},
                  "state": {"type": "string"},
                  "zip": {"type": "string"}
                }
              },
              "estimated_delivery": {
                "type": "string",
                "format": "date"
              }
            }
          }
        },
        {
          "status_code": 404,
          "content_type": "application/json",
          "description": "Tracking number not found"
        },
        {
          "status_code": 401,
          "content_type": "application/json",
          "description": "Authentication required"
        }
      ]
    },
    {
      "path": "/api/v1/shipments",
      "method": "POST",
      "summary": "Create a new shipment",
      "description": "Create a new shipment and receive a tracking number",
      "authentication_required": true,
      "tags": ["shipping"],
      "request": {
        "content_type": "application/json",
        "body_schema": {
          "type": "object",
          "required": ["from", "to", "package"],
          "properties": {
            "from": {
              "type": "object",
              "properties": {
                "name": {"type": "string"},
                "address": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string"},
                "zip": {"type": "string"},
                "phone": {"type": "string"}
              }
            },
            "to": {
              "type": "object",
              "properties": {
                "name": {"type": "string"},
                "address": {"type": "string"},
                "city": {"type": "string"},
                "state": {"type": "string"},
                "zip": {"type": "string"},
                "phone": {"type": "string"}
              }
            },
            "package": {
              "type": "object",
              "properties": {
                "weight": {"type": "number"},
                "dimensions": {
                  "type": "object",
                  "properties": {
                    "length": {"type": "number"},
                    "width": {"type": "number"},
                    "height": {"type": "number"}
                  }
                }
              }
            }
          }
        },
        "parameters": []
      },
      "responses": [
        {
          "status_code": 201,
          "content_type": "application/json",
          "description": "Shipment created successfully",
          "body_schema": {
            "type": "object",
            "properties": {
              "tracking_number": {"type": "string"},
              "shipment_id": {"type": "string"},
              "estimated_delivery": {"type": "string", "format": "date"}
            }
          }
        },
        {
          "status_code": 400,
          "content_type": "application/json",
          "description": "Invalid request data"
        }
      ]
    }
  ],
  "authentication": [
    {
      "type": "api_key",
      "name": "API Key Authentication",
      "description": "Include your API key in the X-API-Key header",
      "location": "header",
      "parameter_name": "X-API-Key"
    }
  ],
  "rate_limits": [
    {
      "requests": 100,
      "period": "1 minute",
      "description": "100 requests per minute per API key"
    },
    {
      "requests": 10000,
      "period": "1 day",
      "description": "10,000 requests per day per API key"
    }
  ],
  "documentation_url": "https://docs.examplecourier.com/api",
  "extracted_at": "2026-01-25T12:00:00"
}
